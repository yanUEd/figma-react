    "serve-dev": "npx http-server test -p 3000 -c-1",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "layout",
