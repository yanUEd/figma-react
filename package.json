{
  "name": "figma-react-layout",
  "version": "1.0.3",
  "description": "AI-native Design-as-Code layout components inspired by Figma Auto Layout",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
